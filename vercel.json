{
    "builds":[
        {
            "src": "bibliotecapi/bibliotecapi/wsgi.py",
            "use": "@vercel/python",
            "config": {"maxLambdaSize": "15mb", "runtime": "python3.9"}
        }
    ],
    "routes": [
        {
            "src": "/bibliotecapi/(.*)",
            "dest": "bibliotecapi/bibliotecapi/wsgi.py"
        }
    ]
}